NODE_ENV=development
PORT=3000
HOST=localhost
FRONTEND_URL=http://localhost:5173

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
SESSION_SECRET=your-session-secret-change-this-too

# Database Configuration
DATABASE_URL=postgres://postgres:password@localhost:5432/plant_disease_db
DB_DIALECT=postgres
DB_HOST=localhost
DB_PORT=5432
DB_NAME=plant_disease_db
DB_USER=postgres
DB_PASSWORD=adam

# Machine Learning Model Paths (klasifikasi-tanaman integration)
# Pointing to the original ML model from klasifikasi-tanaman folder
TFJS_MODEL_PATH=../../klasifikasi-tanaman/tfjs_model/model.json
TFJS_CLASSES_PATH=../../klasifikasi-tanaman/tfjs_model/classes.json

# Upload Configuration
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=jpg,jpeg,png,webp

# Security
BCRYPT_ROUNDS=12
SESSION_TIMEOUT=86400000

# Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=debug
LOG_FILE=./logs/app.log

# CORS
CORS_ORIGINS=http://localhost:5173,http://localhost:3000,http://localhost:3001 